@article{Varoquaux2018,
  TITLE = {Atlases of cognition with large-scale brain mapping},
  AUTHOR = {Varoquaux, Ga{\"e}l and Schwartz, Yannick and Poldrack, Russell and Gauthier, Baptiste and Bzdok, Danilo and Poline, Jean-Baptiste and Thirion, Bertrand},
  JOURNAL = {PLoS Comput Biol},
  PUBLISHER = {Public Library of Science},
  YEAR = {2018},
  PDF = {https://hal.inria.fr/hal-01908189/file/plos_paper.pdf},
  HAL_ID = {hal-01908189},
  HAL_VERSION = {v1},
  VOLUME = {14},
  NUMBER = {11},
  URL = {https://doi.org/10.1371/journal.pcbi.1006565},
  ANNOTATE={* This paper discusses functional atlasing based on large-scale data, such as those available in open repositories (OpenNeuro, NeuroVault). It shows that machine learning models can capture the link between cognitive concepts and brain regions, provided that they rely on the conjunction of two mappings: encoding (brain mapping) and decoding (predictive modeling). This also shows that cognitive ontologies provide an important resource to improve this atlasing endeavour. The paper coins the necessity of validating such models across studies, to avoid fitting the idiosyncrasies of each study.}
}

